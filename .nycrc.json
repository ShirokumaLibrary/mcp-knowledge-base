{
  "all": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/__tests__/**",
    "src/**/__tests__/**",
    "src/test-utils/**",
    "src/**/fixtures/**",
    "src/**/mocks/**",
    "src/**/*-v2.ts",
    "src/**/handler-patterns.ts",
    "src/types/**",
    "src/**/*.d.ts"
  ],
  "reporter": [
    "text",
    "lcov",
    "html"
  ],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80
}